{
  "name": "creative-semantic-store",
  "version": "1.0.0",
  "description": "Data store for creative content using semantic web formats",
  "main": "dist/index.js",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src test",
    "check": "npm run lint -s",
    "deps-check": "dependency-check package.json --entry src",
    "watch": "watch 'npm run build' src test",
    "test": "babel-node test/index.js | tspec",
    "prebuild": "npm run check -s && npm run clean -s",
    "build": "babel src -d dist",
    "postbuild": "npm run test -s",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover test/index.js",
    "coveralls": "npm run coverage -s && coveralls < coverage/lcov.info",
    "postcoveralls": "rimraf ./coverage",
    "prepublish": "npm run build -s",
    "deploy": "git pull --rebase origin master && git push origin master",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "postpublish": "git push origin master --follow-tags",
    "toc": "doctoc --github --title \"# Changelog\" CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flarebyte/creative-semantic-store.git"
  },
  "keywords": [
    "node"
  ],
  "author": "olih <olih@users.noreply.github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flarebyte/creative-semantic-store/issues"
  },
  "homepage": "https://github.com/flarebyte/creative-semantic-store#readme",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.2.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-latest": "^6.24.0",
    "coveralls": "^2.11.4",
    "dependency-check": "^2.5.1",
    "doctoc": "^0.15.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "isparta": "^4.0.0",
    "rimraf": "^2.4.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.2.2",
    "watch": "^0.16.0"
  },
  "dependencies": {
    "ajv": "^4.11.5",
    "fs-extra": "^2.1.2",
    "joi": "^10.3.0",
    "lodash": "^4.17.4",
    "multimap": "^1.0.2",
    "n3": "^0.9.1",
    "string": "^3.3.3"
  }
}
